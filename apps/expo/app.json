{
  "expo": {
    "name": "Body Fitness",
    "slug": "bodyfitnessapp",
    "scheme": "bodyfitnessapp",
    "version": "8.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.akrj.bodyfitnessapp",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.akrj.bodyfitnessapp",
      "versionCode": 8,
      "permissions": ["com.google.android.gms.permission.AD_ID"]
    },
    "owner": "akrj21896",
    "plugins": ["expo-router"],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-6695906984875280~2291755254",
    "ios_app_id": "ca-app-pub-xxxxxxxx~xxxxxxxx",
    "delay_app_measurement_init": true
  }
}
